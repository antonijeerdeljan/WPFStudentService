<Window x:Class="Front.Uklanjanje_predmeta"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Front"
        mc:Ignorable="d"
Title="Uklanjanje predmeta" Height="150" Width="300">
    <Grid>
        <Label Content="Da li ste sigurni da zelite da uklonite predmet?" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,32,0,0"
        />
        <Button Content="Ne" IsCancel="True" HorizontalAlignment="Left" Margin="174,83,0,0" VerticalAlignment="Top" Width="35" Click ="CancelButton_Click"/>
        <Button Content="Da" HorizontalAlignment="Left" Margin="83,83,0,0" VerticalAlignment="Top" Width="35" Click="Ukloni_Button_Click"/>

    </Grid>
</Window>
